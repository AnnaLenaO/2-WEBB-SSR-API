<!Doctype html>
<html>

<head>
    <link rel='stylesheet' type='text/css' href='/static/main.css'>
</head>

<body>
    <h1>The Kino</h1>

    <main>
        <ul>
            <% articles.forEach((article)=> { %>
                <li>
                    <a href=<%='/movies/' +article.id %>
                        <h2>
                            <%= article.title %>
                        </h2>
                    </a>
                </li>
                <hr />
            <% }) %>
        </ul>
    </main>

</body>

</html>